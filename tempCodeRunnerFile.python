fruits = ['apple', 'banana', 'peach', 'apple', 'pear']
counts = {}

# Without .get()
for fruit in fruits:
    if fruit not in counts:
        counts[fruit] = 0
    counts[fruit] += 1

# With .get()
counts_get = {}
for fruit in fruits:
    counts_get[fruit] = counts_get.get(fruit, 0) + 1

print(counts)